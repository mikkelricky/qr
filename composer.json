{
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^2",
        "easycorp/easyadmin-bundle": "^4.9",
        "endroid/qr-code-bundle": "^5.0",
        "stof/doctrine-extensions-bundle": "^1.10",
        "symfony/console": "~7.0.0",
        "symfony/dotenv": "~7.0.0",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "~7.0.0",
        "symfony/runtime": "~7.0.0",
        "symfony/security-bundle": "~7.0.0",
        "symfony/translation": "7.0.*",
        "symfony/yaml": "~7.0.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.5",
        "ergebnis/composer-normalize": "^2.42",
        "friendsofphp/php-cs-fixer": "^3.54",
        "phpstan/phpstan": "^1.10",
        "rector/rector": "^1.0",
        "symfony/maker-bundle": "^1.59",
        "vincentlanglet/twig-cs-fixer": "^2.7"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "endroid/installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "docker": true,
            "require": "7.0.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
